                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
tmp.*

FROM 
(
SELECT 
	Accidents.Accident_Index,
   CONVERT(DATE,TRIM(Accidents.Date),103) as Date,
	Accidents.Time,
	Cast(Accidents.Accident_Severity as int) as Accident_Severity1,
	Cast(Accidents.Road_Type as int) as RoadType1 ,
	Cast(Accidents.Local_Authority_District as int) as Local_Authority_District1 ,
	Cast(Accidents.Urban_or_Rural_Area as int) as Urban_or_Rural_Area1,
	Case when Cast(Accidents.Speed_limit as int) &gt;50 then 'Built up Road' else 'Non Built up Road' end as BuiltupRoad ,--Built up road
	Accidents.LSOA_of_Accident_Location,
	Cast(Accidents.Number_of_Casualties as int) as Number_of_Casualties1,
	Postcodes_Stage.postcode ,
	ROW_NUMBER() OVER ( Partition by
    Accidents.Accident_Index
    ORDER BY geography::Point(Accidents.Latitude, Accidents.Longitude, 4326).STDistance(geography::Point(Postcodes_Stage.Latitude, Postcodes_Stage.Longitude, 4326))) number
FROM(select * from Accidents_Stage  where Accidents_Stage.Flag !=1)
	 as Accidents
	JOIN 
		PCD_LSOA_Stage
		ON Accidents.LSOA_of_Accident_Location = PCD_LSOA_Stage.lsoa11cd 
	JOIN 
		Postcodes_Stage
		ON PCD_LSOA_Stage.pcds like Postcodes_Stage.postcode + ' %'
) tmp 
where tmp.number = 1</property>
